{
  "redirects": [
    {
      "source": "/terms/",
      "destination": "/terms",
      "permanent": true
    },
    {
      "source": "/privacy/",
      "destination": "/privacy",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/terms",
      "destination": "/index.html"
    },
    {
      "source": "/privacy",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}