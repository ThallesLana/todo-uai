{
  "redirects": [
    {
      "source": "/terms/",
      "destination": "/terms",
      "permanent": true
    },
    {
      "source": "/privacy/",
      "destination": "/privacy",
      "permanent": true
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}